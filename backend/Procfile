web: gunicorn ecom_project.wsgi:application --bind 0.0.0.0:$PORT --chdir backend
release: python manage.py makemigrations users --noinput --chdir backend && python manage.py makemigrations products --noinput --chdir backend && python manage.py makemigrations orders --noinput --chdir backend && python manage.py makemigrations notifications --noinput --chdir backend && python manage.py makemigrations test_app --noinput --chdir backend && python manage.py migrate --chdir backend && python manage.py loaddata products/fixtures/coupons.json --chdir backend && python manage.py clear_cache --chdir backend && python manage.py collectstatic --no-input --chdir backend
